{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "@react-native-community",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "quoteProps": "consistent",
                "singleQuote": true,
                "tabWidth": 2,
                "trailingComma": "es5",
                "useTabs": false
            }
        ]
    }
}
